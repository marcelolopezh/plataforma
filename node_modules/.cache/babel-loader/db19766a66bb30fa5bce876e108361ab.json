AAA,QAAA,KAAA,EAAA;AAAA,OATA,CAVA;AAqBA,MAAA,MAAA,EAAA;AArBA,KAAA;AAuBA,GAzBA;AA0BA,EAAA,OA1BA,qBA0BA;AACA,SAAA,SAAA;AACA,GA5BA;AA6BA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,uBACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA,sBAEA,KAAA,CAAA,SAAA,IAAA,KAAA,CAAA,KAAA,IAAA,KAAA,CAAA,MAAA,IAAA,KAAA,CAAA,KAFA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAGA,EAAA,CAAA,UAAA,CAAA,QAAA,EAAA,GAAA,CAAA;AACA,kBAAA,SAAA,EAAA,KAAA,CAAA,SADA;AAEA,kBAAA,WAAA,EAAA,KAAA,CAAA,KAFA;AAGA,kBAAA,MAAA,EAAA,KAAA,CAAA,MAHA;AAIA,kBAAA,KAAA,EAAA,KAAA,CAAA;AAJA,iBAAA,CAHA;;AAAA;AAUA,gBAAA,KAAA,CAAA,SAAA,GAVA,CAUA;;;AACA,gBAAA,KAAA,CAAA,SAAA,GAXA,CAWA;;;AAXA;AAAA;;AAAA;AAcA,gBAAA,OAAA,CAAA,GAAA,CAAA,eAAA;;AAdA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAiBA,gBAAA,OAAA,CAAA,GAAA;;AAjBA;AAmBA,gBAAA,KAAA,CAAA,SAAA;;AAnBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoBA,KArBA;AAsBA,IAAA,SAtBA,uBAsBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,EAAA,CAAA,UAAA,CAAA,QAAA,EAAA,GAAA,EAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,KAHA,GAGA,EAHA;AAIA,gBAAA,QAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,sBAAA,SAAA,GAAA,GAAA,CAAA,IAAA,EAAA;AACA,kBAAA,SAAA,CAAA,EAAA,GAAA,GAAA,CAAA,EAAA;AACA,kBAAA,KAAA,CAAA,IAAA,CAAA,SAAA,EAHA,CAGA;AACA,iBAJA;AAKA,gBAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AATA;AAAA;;AAAA;AAAA;AAAA;AAWA,gBAAA,OAAA,CAAA,GAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KAnCA;AAoCA,IAAA,WApCA,uBAoCA,IApCA,EAoCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AAFA;AAAA,uBAGA,EAAA,CAAA,UAAA,CAAA,QAAA,EAAA,GAAA,CAAA,IAAA,CAAA,EAAA,EAAA,MAAA,EAHA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAKA,gBAAA,OAAA,CAAA,GAAA;;AALA;AAOA,gBAAA,MAAA,CAAA,SAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KA5CA;AA6CA,IAAA,SA7CA,uBA6CA;AACA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,KAhDA;AAiDA,IAAA,SAjDA,uBAiDA;AACA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,WAAA,KAAA,GAAA,IAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA;AAvDA;AA7BA,CAAA","sourcesContent":["<template v-slot:default>\r\n    <div>\r\n        <v-simple-table>\r\n            <thead>\r\n            <tr>\r\n                <th style=\"display:hidden\">#</th>\r\n                <th class=\"text-left\">Encargado</th>\r\n                <th class=\"text-left\">Tarea</th>\r\n                <th class=\"text-left\">Estado</th>\r\n                <th class=\"text-left\">Horas</th>\r\n                <th class=\"text-left\">Acciones</th>\r\n            </tr>\r\n            </thead>\r\n            <tbody>\r\n            <tr v-for=\"(item,index) in tareas\" :key=\"item.id\">\r\n                <td>{{ index+1 }}</td>\r\n                <td>{{ item.encargado }}</td>\r\n                <td>{{ item.descripcion }}</td>\r\n                <td>{{ item.estado }}</td>\r\n                <td>{{ item.horas }}</td>\r\n                <td>\r\n                <v-btn class=\"mx-2\" fab dark small color=\"teal\">\r\n                    <v-icon dark @click=\"deleteTarea(item)\">mdi-delete</v-icon>\r\n                </v-btn>\r\n            </td>\r\n            </tr>\r\n            </tbody>\r\n            <v-btn class=\"mx-2 text-center\" fab dark small color=\"indigo\" @click=\"dialog = true\">\r\n                <v-icon dark>mdi-plus</v-icon>\r\n            </v-btn>\r\n        </v-simple-table>\r\n        <!-- MODAL PARA AGREGAR TAREAS  -->\r\n        <v-dialog max-width=\"50%\" v-model=\"dialog\">\r\n            <v-card>\r\n                <v-container>\r\n                    <h1>Agregar Tarea</h1>\r\n                    <v-form @submit.prevent=\"\">\r\n                        <v-text-field type=\"text\" label=\"Nombre Encargado\" v-model=\"encargado\"></v-text-field>\r\n                        <v-text-field type=\"text\" label=\"Tarea\" v-model=\"tarea\"></v-text-field>\r\n                        <v-text-field type=\"text\" label=\"Estado\" v-model=\"estado\"></v-text-field>\r\n                        <v-text-field type=\"number\" label=\"Horas Estimadas\" v-model=\"horas\"></v-text-field>\r\n                        <v-btn\r\n                            color=\"success\"\r\n                            class=\"mr-4\"\r\n                            @click=\"setTareas()\"\r\n                        > \r\n                            Ingresar Tarea\r\n                        </v-btn>\r\n                        <v-btn\r\n                            color=\"error\"\r\n                            class=\"mr-4\"\r\n                            @click=\"stopModal()\"\r\n                        > \r\n                            Cerrar\r\n                        </v-btn>\r\n                    </v-form>\r\n                </v-container>\r\n            </v-card>\r\n        </v-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport {db} from '../main'\r\nexport default {\r\n   data : function(){\r\n        return {\r\n\r\n            // VARIABLES PARA AGREGAR UN EVENTO //\r\n            dialog : false,\r\n            encargado : null,\r\n            tarea : null,\r\n            estado : null,\r\n            horas : null,\r\n            /////////////////////////////////////\r\n\r\n            headers : [\r\n            {\r\n                text : \"Descripcion\",\r\n                align : \"start\",\r\n                sortable : false,\r\n                value : \"descripcion\"\r\n            },\r\n            {text:'Encargado', value : 'encargado'},\r\n            {text:'Estado', value : 'estado'},\r\n            {text:'Horas', value : 'horas'}\r\n            ],\r\n            tareas : []\r\n        }\r\n   },\r\n   mounted(){\r\n       this.getTareas();\r\n   },\r\n   methods : {\r\n       async setTareas(){\r\n           try{\r\n               if(this.encargado && this.tarea && this.estado && this.horas){\r\n                   await db.collection('tareas').add({\r\n                       encargado : this.encargado,\r\n                       descripcion : this.tarea,\r\n                       estado : this.estado,\r\n                       horas : this.horas\r\n                   });\r\n\r\n                    this.getTareas() // -> Cuando se agreguen los datos, se vuelve a llenar nuestro array\r\n                    this.clearVars() // -> Limpiamos las variables\r\n\r\n               }else{\r\n                   console.log(\"Campos Vacios\")\r\n               }\r\n           }catch(error){\r\n               console.log(error)\r\n           }\r\n           this.stopModal();\r\n       },\r\n       async getTareas(){\r\n           try{\r\n               const snapshot = await db.collection('tareas').get();\r\n               const datos = [];\r\n               snapshot.forEach(doc => {\r\n                   let tareaData = doc.data();\r\n                   tareaData.id = doc.id;\r\n                   datos.push(tareaData); // GUARDAMOS LA INFO DE LA DB EN TAREAS ARRAY\r\n               })\r\n               this.tareas = datos\r\n           } catch (error){\r\n               console.log(error)\r\n           }\r\n       },\r\n       async deleteTarea(item){\r\n           try{\r\n               console.log(item)\r\n               await db.collection('tareas').doc(item.id).delete();\r\n           }catch(error){\r\n               console.log(error)\r\n           }\r\n           this.getTareas();\r\n       },\r\n       stopModal(){\r\n           //FUNCION QUE CONDICIONA APERTURA DE MODAL\r\n           this.dialog = false\r\n       },\r\n       clearVars(){\r\n           //LIMPIAMOS LAS VARIABLES PARA QUE EN EL MODAL NO APAREZCAN ESCRITAS\r\n           this.tareas = null\r\n           this.encargado = null\r\n           this.horas = null\r\n           this.descripcion = null\r\n       }\r\n   }\r\n}\r\n</script>"],"sourceRoot":"src/components"}]}